{
    "name": "decodelabs/eventful",
    "description": "Asynchronous IO event dispatcher",
    "type": "library",
    "keywords": [ "events", "io" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/coercion": "^0.3",
        "decodelabs/deliverance": "^0.3",
        "decodelabs/exceptional": "^0.5.2"
    },
    "require-dev": {
        "ext-event": "*",

        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "suggest": {
        "ext-event": "Faster and more reliable event loops"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Eventful\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Eventful\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
